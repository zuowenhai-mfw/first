.clearfix:after {
    content: "";
    height: 0;
    line-height: 0;
    display: block;
    visibility: hidden;
    clear: both;
}
.clearfix {
    zoom: 1;
}
.fl {
    float: left;
}
.fr {
    float: right;
}
.txt-rht {
    text-align: right;
    padding-right: 5px;
}
.tab-w {
    padding: 25px;
}
ul {
    -webkit-padding-start: 0;
    -moz-padding-start: 0;
}
ul,li {
    list-style: none;
}
.table {
    margin-bottom: 0;
}
.panel-body {
    padding: 0;
}
.table th,
.table td {
    text-align: center;
}
.tab-title {
    height: auto;
    margin-bottom: 0;
}
.tab-title li {
    float: left;
    padding: 5px 2%;
    text-align: center;
    cursor: pointer;
}
